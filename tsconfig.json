{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["DOM", "DOM.Iterable", "ES2021"],
		"module": "ESNext",
		"jsx": "react-jsx",
		"strict": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"moduleResolution": "bundler",
		"paths": {
			"*": ["./@mf-types/*"],
			"@/*": ["./src/*"]
		}
		// "plugins": [{ "name": "typescript-plugin-css-modules" }]
	},
	"include": ["src", "environment.d.ts", "declaration.d.ts"]
}
